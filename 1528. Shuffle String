
class Solution {
public:
    string restoreString(string s, vector<int>& indices) {
        string t=s;
        for(int i=0; i<s.size(); i++){
           t[indices[i]]=s[i];
        }
        return t;
    }
};

/*

Runtime: 4 ms, faster than 94.61% of C++ online submissions for Shuffle String.
Memory Usage: 15.2 MB, less than 42.77% of C++ online submissions for Shuffle String.

*/
